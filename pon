import java.io.*;
import java.util.*;
class pon
{
    public static void main(String args[])
    {   int a;
        Scanner sc=new Scanner(System.in);
        a=sc.nextInt();
        if(a>0)
        {
            System.out.println("The number is Positive");
        }
        else if(a<0)
        {
            System.out.println("The number is Negative");
            
        }
        else
        {
            System.out.println("The number is zero");
        }
        
    }
}

